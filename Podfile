# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'Starter_IOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks

    source 'https://github.com/CocoaPods/Specs.git'
    source 'https://gitlab.com/wavelabs/idn-sdk-ios.git'

    use_frameworks!

  # Pods for Starter_IOS
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'
    pod 'Google/SignIn'

    pod 'idn_sdk_ios', :git => 'https://gitlab.com/wavelabs/idn-sdk-ios.git'



  target 'Starter_IOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Starter_IOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
